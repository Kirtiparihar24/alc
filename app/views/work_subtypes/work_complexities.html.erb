<div class="table_wrapper" style="padding: 16px">
  <div>
    <table cellpadding="0" cellspacing="0" width="100%" border="1">
      <tbody>
        <tr>
          <th width="50%">Open Task</th>
          <th width="50%">Complete Task</th>
        </tr>
        <tr>
          <td width="50%">
            <table style="margin-left:12px;">
              <tr>
                <td>
                  <ul>
                    <% @open_tasks.each do |task| %>
                      <li>
                        <%= task.name rescue '-'%>
                      </li>
                    <%end%>
                  </ul>
                  <%= "NULL" if @open_tasks.blank?  %>
                </td>
              </tr>
            </table>
          </td>
          <td width="50%">
            <table style="margin-left:12px;">
              <tr>
                <td>
                  <ul>
                    <% @complete_tasks.each do |task| %>
                      <li>
                        <%= task.name rescue '-'%>
                      </li>
                    <%end%>
                  </ul>
                  <%= "NULL" if @complete_tasks.blank?  %>
                </td>
              </tr>
            </table>
          </td>
        </tr>
      </tbody>
    </table>
    <br/>
    <% if params[:class_type] == "WorkSubtype" %>
      <span><input type="button" value="Remove Work Sub-Type" onclick = "delete_work_subtype('<%=params[:id]%>',true);" /></span>
    <%else%>
      <span><input type="button" value="Remove complexity" onclick = "jQuery('#<%=params[:id]%>').remove();tb_remove();return false;" /></span>
    <%end%>
    <span><input type="button" name="Cancel" value="Cancel" onclick="tb_remove();return false;"/></span>
  </div>
</div>