   contactSearch.children("input").val('<%=@contact.full_name.try(:titleize)  unless @contact.nil?%>');
}else{
 jQuery('#_contact_ctl').val('<%=@contact.full_name.try(:titleize)  unless @contact.nil?%>');
}
