<% formINDEX = @formINDEX if @formINDEX%>
<% if spanid == "#popup_cnt_#{formINDEX}_span" %>
  <%=custom_select_tag "note[contact_id]", options_for_select(@contacts.collect{|cnt|[cnt.full_name, cnt.id]}, contact_id.to_i) ,{:prompt => "",:style=>"width:223px",:onchange=>"getMattersContacts('#{formINDEX}','#popup_mtr_#{formINDEX}_span','#popup_cnt_#{formINDEX}_span','',this.value)"}%>
<% elsif spanid == "#old_cnt_span" %>
  <%=custom_select_tag "note[contact_id]", options_for_select(@contacts.collect{|cnt|[cnt.full_name, cnt.id]}, contact_id.to_i) ,{:prompt => "",:style=>"width:100px",:onchange=>"getMattersContacts('#{formINDEX}','#old_mtr_span','#old_cnt_span','',this.value)"}%>
<% elsif spanid == "#comm_cnt_#{formINDEX}_span" %>
  <%=custom_select_tag "note[contact_id]", options_for_select(@contacts.collect{|cnt|[cnt.full_name, cnt.id]}, contact_id.to_i) ,{:prompt => "",:class=>"dropbox1",:onchange=>"getMattersContacts('#{formINDEX}','#comm_mtr_#{formINDEX}_span','#comm_cnt_#{formINDEX}_span','',this.value)"}%>
<% elsif spanid == "#_contact_ctl_#{formINDEX}" %>
  <% @contacts.each do |cnt_name| %>
    <input  type="text" id="comm_cnt_<%=formINDEX %>_span" title='Search' size="15" value="<%= cnt_name.full_name%>" onkeyup="search_comm_matter(this.value,'<%= formINDEX%>','comm_cnt_<%=formINDEX%>_span','_matter_ctl_<%= formINDEX%>','contacts')" ></input>
  <% end %>
<% else %>
  <%=custom_select_tag "note[contact_id]", options_for_select(@contacts.collect{|cnt|[cnt.full_name, cnt.id]}, contact_id.to_i) ,{:prompt => "",:style=>"width:223px",:onchange=>"getMattersContacts('#{formINDEX}','#home_mtr_#{formINDEX}_span','#home_cnt_#{formINDEX}_span','',this.value)"}%>
<% end %>