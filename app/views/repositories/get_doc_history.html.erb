<div class="gridDiv" style="width:920px">
  <div class="titleHead">
     <div class="fl"><%=t(:text_versions_of_document)%></div>
    <div class="fr ptr5"><a href="#"><img src="/images/livia_portal/icon_close)%>.gif" alt="close" title="close" onclick="closeFaceBox();" /></a></div>
    <br class="clear"/>
  </div>
  <div class="windowForm">
    <div>
      <table cellpadding="0" cellspacing="0" border="0" align="center" style="width:100%;">
        <tr>
          <th width='20%'><%=t(:text_document_name)%></th>
          <th width='20%'><%=t(:text_file_name)%></th>
          <th width='15%'><%=t(:text_uploaded_on)%></th>
          <th width='45%'><%=t(:text_uploaded_by)%></th>
        </tr>
        <tbody class="iframeview">
          <%@documents.each do |document|%>
             <%if document.assets[0].parent_id.nil?%>
                 <tr class="<%=cycle("bg1", "bg2")%>">
                    <td class="txtL"><%=document.name%></td>
                    <td><%=link_to(document.assets[0].data_file_name, asset_path(document.assets[0]))%></td>
                    <td><%=document.created_at.to_time.strftime('%m/%d/%y %H :%M')%></td>
                    <td> <%=raw get_uploaded_by_details(@doc_home,document) %> </td>
                 </tr>
              <%end%>
         <%end%>
        </tbody>
      </table>
    </div>
  </div>
</div>